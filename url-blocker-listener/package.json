{
  "name": "url-blocker-listener",
  "version": "1.0.0",
  "description": "Express server to listen for blocked URL requests from Chrome plugin",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "pkg . --targets node18-win-x64,node18-linux-x64,node18-macos-x64 --out-path dist",
    "build:win": "pkg . --targets node18-win-x64 --out-path dist",
    "build:linux": "pkg . --targets node18-linux-x64 --out-path dist",
    "build:macos": "pkg . --targets node18-macos-x64 --out-path dist",
    "build:all": "pkg . --targets node18-win-x64,node18-linux-x64,node18-macos-x64 --out-path dist"
  },
  "bin": "server.js",
  "pkg": {
    "assets": [
      "app.properties"
    ],
    "outputPath": "dist"
  },
  "keywords": ["express", "chrome-extension", "url-blocker"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "properties-reader": "^2.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "pkg": "^5.8.1"
  }
}
